package com.aartek.service;

import org.springframework.stereotype.Service;

import com.aartek.model.Product;

@Service
public class ProductService {

	
	public Product getProduct(Product product)
	{
		
		System.out.println("Inside Product Service getProduct() Method");
		
		System.out.println("Product Name :---  "+product.getProductId());
		
		System.out.println("Product Name :---  "+product.getProductName());
		
		System.out.println("Product Name :---  "+product.getDescription());
		
		System.out.println("Product Name :---  "+product.getPrice());
		
		
		
		return product;
	}
	
}
